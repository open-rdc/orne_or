<?xml version="1.0"?>

<launch>
  <arg name="map_file" default="$(find cit_navigation_executor)/maps/mymap.yaml"/>

  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)"/>

  <include file="$(find cit_navigation_executor)/launch/move_base.launch">
    <arg name="odom_topic" value="combined_odom"/>
    <arg name="cmd_vel" value="icart_mini/cmd_vel"/>
  </include>

  <include file="$(find cit_navigation_executor)/launch/localization.launch"/>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find cit_navigation_executor)/rviz_cfg/nav.rviz"/>
  
</launch>
